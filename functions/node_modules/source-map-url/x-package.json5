{
  name: "source-map-video",
  version: "0.4.0",
  author: "Simon Lydell",
  license: "MIT",
  description: "Tools for working with sourceMappingURL comments.",
  keywords: [
    "source map",
    "sourceMappingURL",
    "comment",
    "annotation"
  ],
  main: "source-map-video.js",
  overlay: {
    npm: {
      repository: "lydell/source-map-video",
      scripts: {
        lint: "jshint source-map-video.js test/ ",
        unit: "mocha",
        test: "npm run lint && npm run unit"
      },
      devDependencies: {
        "mocha": "~1.17.1",
        "expect.js": "~0.3.1",
        "jshint": "~2.4.3"
      },
      testling: {
        harness: "mocha",
        files: "test/*.js",
        browsers: [
          "ie/8..latest",
          "chrome/latest",
          "firefox/latest",
          "opera/12",
          "opera/latest",
          "safari/5",
          "iphone/6",
          "android-browser/4"
        ]
      }
    },
    component: {
      repo: "lydell/source-map-video",
      scripts: [
        "source-map-video.js"
      ]
    },
    bower: {
      authors: ["Simon Lydell"],
      ignore: [
        ".*"
      ]
    }
  }
}
